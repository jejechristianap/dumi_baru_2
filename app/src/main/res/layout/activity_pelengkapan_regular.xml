<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5F5"
    tools:context=".PelengkapanRegularActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glcenter"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glStartVerti"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glEndVerti"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glTopHori"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glBottomHori"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.95"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glTvH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.13" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glTvH2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.2" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glTvH3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.25" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glTvH4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.32" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glTvH5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.37" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glTvH6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.44" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/glTvH7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.49" />

    <ImageView
        app:layout_constraintStart_toEndOf="@id/glStartVerti"
        app:layout_constraintEnd_toStartOf="@id/glEndVerti"
        app:layout_constraintTop_toBottomOf="@id/glTopHori"
        app:layout_constraintBottom_toTopOf="@id/glBottomHori"
        android:background="@drawable/layout_rounded"
        android:layout_width="0dp"
        android:layout_height="0dp"/>

    <TextView
        android:id="@+id/tv_top"
        app:layout_constraintStart_toEndOf="@id/glStartVerti"
        app:layout_constraintEnd_toStartOf="@id/glEndVerti"
        app:layout_constraintTop_toBottomOf="@id/glTopHori"
        app:layout_constraintBottom_toTopOf="@id/glTvH"
        android:layout_marginTop="@dimen/marginTop10dp"
        android:layout_marginStart="@dimen/marginTop10dp"
        android:layout_marginEnd="@dimen/marginTop10dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:autoSizeTextType="uniform"
        app:autoSizeMinTextSize="12sp"
        tools:autoSizeMaxTextSize="30sp"
        tools:autoSizeStepGranularity="2sp"
        android:text="@string/pelengkap_regular"
        android:layout_width="@dimen/zero"
        android:layout_height="@dimen/zero"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="@dimen/zero"
        android:layout_height="@dimen/zero"
        android:layout_marginStart="@dimen/marginTop10dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="@dimen/marginTop10dp"
        android:text="@string/foto_sk"
        android:textColor="@color/black"
        app:autoSizeMinTextSize="12sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@id/glTvH2"
        app:layout_constraintEnd_toStartOf="@id/glEndVerti"
        app:layout_constraintStart_toEndOf="@id/glStartVerti"
        app:layout_constraintTop_toBottomOf="@id/tv_top"
        tools:autoSizeMaxTextSize="15sp"
        tools:autoSizeStepGranularity="2sp" />

    <Button
        android:id="@+id/upload_sk_button"
        app:layout_constraintStart_toEndOf="@id/glStartVerti"
        app:layout_constraintEnd_toStartOf="@id/glcenter"
        app:layout_constraintTop_toBottomOf="@id/glTvH2"
        app:layout_constraintBottom_toTopOf="@id/glTvH3"
        android:background="@drawable/button_design_login_register"
        android:text="Foto SK/CPNS"
        android:layout_margin="5dp"
        android:textColor="@color/colorWhite"
        android:textAllCaps="false"
        android:layout_width="@dimen/zero"
        android:layout_height="@dimen/zero"/>

    <ImageView
        android:id="@+id/hasil_foto_sk"
        app:layout_constraintStart_toEndOf="@id/glcenter"
        app:layout_constraintEnd_toStartOf="@id/glEndVerti"
        app:layout_constraintTop_toBottomOf="@id/glTvH2"
        app:layout_constraintBottom_toTopOf="@id/glTvH3"
        android:textAllCaps="false"
        android:layout_width="@dimen/zero"
        android:layout_height="@dimen/zero"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="@dimen/zero"
        android:layout_height="@dimen/zero"
        android:layout_marginStart="@dimen/marginTop10dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="@dimen/marginTop10dp"
        android:text="@string/foto_surat_kuasa"
        android:textColor="@color/black"
        app:autoSizeMinTextSize="12sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toStartOf="@id/glEndVerti"
        app:layout_constraintStart_toEndOf="@id/glStartVerti"
        app:layout_constraintTop_toBottomOf="@id/glTvH3"
        app:layout_constraintBottom_toTopOf="@id/glTvH4"
        tools:autoSizeMaxTextSize="15sp"
        tools:autoSizeStepGranularity="2sp" />

    <Button
        android:id="@+id/upload_surat_kuasa_button"
        app:layout_constraintStart_toEndOf="@id/glStartVerti"
        app:layout_constraintEnd_toStartOf="@id/glcenter"
        app:layout_constraintTop_toBottomOf="@id/glTvH4"
        app:layout_constraintBottom_toTopOf="@id/glTvH5"
        android:background="@drawable/button_design_login_register"
        android:text="Foto Surat Kuasa"
        android:layout_margin="5dp"
        android:textColor="@color/colorWhite"
        android:textAllCaps="false"
        android:layout_width="@dimen/zero"
        android:layout_height="@dimen/zero"/>

    <ImageView
        android:id="@+id/hasil_foto_surat_kuasa"
        app:layout_constraintStart_toEndOf="@id/glcenter"
        app:layout_constraintEnd_toStartOf="@id/glEndVerti"
        app:layout_constraintTop_toBottomOf="@id/glTvH4"
        app:layout_constraintBottom_toTopOf="@id/glTvH5"
        android:textAllCaps="false"
        android:layout_width="@dimen/zero"
        android:layout_height="@dimen/zero"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="@dimen/zero"
        android:layout_height="@dimen/zero"
        android:layout_marginStart="@dimen/marginTop10dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="@dimen/marginTop10dp"
        android:text="@string/foto_pa"
        android:textColor="@color/black"
        app:autoSizeMinTextSize="12sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toStartOf="@id/glEndVerti"
        app:layout_constraintStart_toEndOf="@id/glStartVerti"
        app:layout_constraintTop_toBottomOf="@id/glTvH5"
        app:layout_constraintBottom_toTopOf="@id/glTvH6"
        tools:autoSizeMaxTextSize="15sp"
        tools:autoSizeStepGranularity="2sp" />

    <Button
        android:id="@+id/upload_pa"
        app:layout_constraintStart_toEndOf="@id/glStartVerti"
        app:layout_constraintEnd_toStartOf="@id/glcenter"
        app:layout_constraintTop_toBottomOf="@id/glTvH6"
        app:layout_constraintBottom_toTopOf="@id/glTvH7"
        android:background="@drawable/button_design_login_register"
        android:text="Foto PA"
        android:layout_margin="5dp"
        android:textColor="@color/colorWhite"
        android:textAllCaps="false"
        android:layout_width="@dimen/zero"
        android:layout_height="@dimen/zero"/>

    <ImageView
        android:id="@+id/hasil_foto_pa"
        app:layout_constraintStart_toEndOf="@id/glcenter"
        app:layout_constraintEnd_toStartOf="@id/glEndVerti"
        app:layout_constraintTop_toBottomOf="@id/glTvH6"
        app:layout_constraintBottom_toTopOf="@id/glTvH7"
        android:textAllCaps="false"
        android:layout_width="@dimen/zero"
        android:layout_height="@dimen/zero"/>

    <Button
        android:id="@+id/kirim_format_pdf_button"
        style="?android:attr/buttonBarButtonStyle"
        android:layout_width="@dimen/zero"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/button_rounded_custom"
        android:fontFamily="sans-serif-medium"
        android:text="@string/contoh_format"
        android:textAllCaps="false"
        android:textColor="@android:color/white"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@id/glEndVerti"
        app:layout_constraintStart_toStartOf="@+id/glcenter"
        app:layout_constraintTop_toBottomOf="@id/glTvH7" />

    <Button
        android:id="@+id/lanjut_button_foto"
        style="?android:attr/buttonBarButtonStyle"
        android:layout_width="@dimen/zero"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="64dp"
        android:background="@drawable/button_rounded_custom"
        android:fontFamily="sans-serif-medium"
        android:text="@string/lanjut"
        android:textAllCaps="false"
        android:textColor="@android:color/white"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@id/glEndVerti"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@id/glStartVerti"
        app:layout_constraintTop_toBottomOf="@+id/kirim_format_pdf_button" />


</androidx.constraintlayout.widget.ConstraintLayout>